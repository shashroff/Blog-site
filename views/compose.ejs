<%- include("partials/header") -%>

<h1>Compose</h1>


<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="inputTitle" autocomplete="off">
    <label>Post</label>
    <textarea class="form-control" name="inputPost" rows="8" cols="80"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>


